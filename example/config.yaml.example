#
# copy this file to config.yaml and 
# add your configuration
#
config:
  basedomain: cluster.example.org
  github:
    client-id: <client id generated by github>
    client-secret: <client secret generated by github>
    org: <github organization>

  kubectl:
    client_secret: <client secret of your choice>

  static:
     email: admin@example.org
     password: <your password>

#
# optional
#
  images:
     kubectlapp: <image name>
     dex: <image name>
  dns_annotation: <your favorite service dns annotation>
